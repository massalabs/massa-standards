{
    "targets": {
        "debug": {
            "outFile": "build/main.wasm",
            "sourceMap": true,
            "debug": true,
            "transform": ["@massalabs/as-transformer"]
        },
        "release": {
            "outFile": "build/main.wasm",
            "sourceMap": true,
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": false,
            "noAssert": false,
            "transform": ["@massalabs/as-transformer"]
        }
    },
    "options": {
        "exportRuntime": true,
        "bindings": "esm"
    }
}
